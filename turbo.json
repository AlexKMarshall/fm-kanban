{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "dependsOn": ["^db:generate"],
      "persistent": true,
      "cache": false
    },
    "start": {
      "dependsOn": ["build"],
      "persistent": true
    },
    "build": {
      "dependsOn": ["^db:generate"],
      "outputs": ["build/**"]
    },
    "typecheck": {
      "dependsOn": ["^db:generate"]
    },
    "knip": {},
    "lint": {},
    "format": {},
    "db:generate": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "db:deploy": {},
    "test:e2e": {},
    "test:e2e:dev": {
      "persistent": true
    }
  }
}
